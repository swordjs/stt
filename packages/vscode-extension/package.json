{
  "name": "stt-translate",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./lib/index.cjs",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib",
    "assets"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.translateGitMessage",
        "title": "stt translate",
        "icon": {
          "light": "assets/translate_dark.png",
          "dark": "assets/translate_light.png"
        }
      }
    ],
    "menus": {
      "scm/title": [
        {
          "when": "scmProvider == git",
          "command": "extension.translateGitMessage",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "dev": "nodemon --watch src/ -C -e ts --debug -x 'npm run build'",
    "build": "unbuild"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0"
  },
  "dependencies": {
    "@swordjs/stt-core": "workspace:*",
    "vscode-nls-i18n": "^0.2.4"
  }
}
