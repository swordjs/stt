{
  "name": "stt-translate",
  "displayName": "stt-translate",
  "publisher": "swordjs",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./out/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib",
    "assets"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.translateGitMessage",
        "title": "stt translate",
        "icon": {
          "light": "assets/translate_dark.png",
          "dark": "assets/translate_light.png"
        }
      }
    ],
    "menus": {
      "scm/title": [
        {
          "when": "scmProvider == git",
          "command": "extension.translateGitMessage",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0"
  },
  "dependencies": {
    "@swordjs/stt-core": "workspace:*",
    "vscode-nls-i18n": "^0.2.4"
  }
}
